@model  IEnumerable<Slot>

<div class="bg-primary m-1 p-1 text-white"><h4>Slots</h4></div>

<table class="table table-sm table-bordered">
    <tr>
    <th>No.</th>
    <th>Contracts:</th>
    <th></th></tr>
    @if (Model.Count() == 0)
    {
        <tr><td colspan="2" class="text-center">No customers.</td></tr>
    }
    else
    {
        foreach (Slot slot in Model)
        {
            <tr>
                <td>@slot.Id</td>
                <td></td>
                <td></td>
            </tr>
        }
    }
</table>
<form asp-action="CreateSlot" method="post" asp-antiforgery="true">
    <button type="submit" class="btn btn-primary">Create Slot</button>
</form>